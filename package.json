{
  "name": "react-select-filter-box",
  "version": "0.0.0",
  "license": "EUPL-1.2",
  "description": "A Sequential Filter Box component for React that provides a Select2-like single-line filter expression builder",
  "keywords": [
    "react",
    "filter",
    "filter-box",
    "select",
    "autocomplete",
    "search",
    "expression-builder",
    "query-builder"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/react-select-filter-box.git"
  },
  "type": "module",
  "main": "./dist/react-select-filter-box.cjs",
  "module": "./dist/react-select-filter-box.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-select-filter-box.mjs",
      "require": "./dist/react-select-filter-box.cjs"
    },
    "./styles": {
      "import": "./dist/styles/style.css",
      "require": "./dist/styles/style.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run --project unit",
    "test:watch": "vitest --project unit",
    "test:all": "vitest run",
    "test:coverage": "vitest run --project unit --coverage",
    "test:storybook": "vitest run --project storybook",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "date-fns": "^4.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18",
    "storybook": "^10.2.3",
    "@storybook/react-vite": "^10.2.3",
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-vitest": "^10.2.3",
    "@storybook/addon-a11y": "^10.2.3",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/addon-onboarding": "^10.2.3",
    "eslint-plugin-storybook": "^10.2.3",
    "playwright": "^1.58.0",
    "@vitest/browser-playwright": "^4.0.18"
  }
}
